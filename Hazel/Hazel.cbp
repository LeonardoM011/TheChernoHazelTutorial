				<Option object_output="bin/Debug/" />
				<Option type="2" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-std=c++11" />
					<Add option="-g" />
					<Add option="-DHZ_PLATFORM_LINUX" />
